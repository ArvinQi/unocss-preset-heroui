{
  "name": "unocss-preset-shadcn",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@9.1.0",
  "description": "use shadcn ui with unocss",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/unocss-preset-shadcn#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyoban/unocss-preset-shadcn"
  },
  "bugs": "https://github.com/hyoban/unocss-preset-shadcn/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "simple-git-hooks && nr build",
    "prepublishOnly": "nr build",
    "release": "release-it",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "unocss": ">= 0.56.0 < 1",
    "unocss-preset-animations": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/node": "^20.12.11",
    "@unocss/preset-mini": "^0.60.0",
    "eslint": "^9.2.0",
    "eslint-config-hyoban": "^2.2.11",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.0",
    "release-it": "^17.2.1",
    "release-it-pnpm": "^4.5.1",
    "rimraf": "^5.0.6",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "unocss": "^0.60.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
