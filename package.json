{
  "name": "unocss-preset-nextui",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@9.5.0",
  "description": "use nextui with unocss",
  "author": "Stephen Zhou <arvinqi@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/ArvinQi/unocss-preset-nextui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArvinQi/unocss-preset-nextui"
  },
  "bugs": "https://github.com/ArvinQi/unocss-preset-nextui/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "simple-git-hooks && nr build",
    "prepublishOnly": "nr build",
    "release": "release-it",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "color": "^4.2.3",
    "color2k": "^2.0.3",
    "deepmerge": "^4.3.1",
    "flat": "^6.0.1"
  },
  "peerDependencies": {
    "unocss": ">= 0.56.0 < 1",
    "unocss-preset-animations": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.14.11",
    "@unocss/preset-mini": "^0.61.5",
    "eslint": "^9.7.0",
    "eslint-config-hyoban": "3.0.0-beta.30",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.61.5",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
